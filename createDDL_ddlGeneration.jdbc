CREATE TABLE USUARIO (ID  SERIAL NOT NULL, EMAIL VARCHAR(50), FIRSTNAME VARCHAR(50), LASTNAME VARCHAR(50), PASSWORD VARCHAR(64), USERNAME VARCHAR(50) NOT NULL, ENDERECO_ID INTEGER, PRIMARY KEY (ID))
CREATE TABLE ENDERECO (ID  SERIAL NOT NULL, CITY VARCHAR(50), COUNTRY VARCHAR(50), STREET VARCHAR(50), SUBURB VARCHAR(50), PRIMARY KEY (ID))
ALTER TABLE USUARIO ADD CONSTRAINT FK_USUARIO_ENDERECO_ID FOREIGN KEY (ENDERECO_ID) REFERENCES ENDERECO (ID)
